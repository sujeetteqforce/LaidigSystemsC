
@{
    ViewBag.Title = "OnSiteInstant";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table width="100%">
    <tr>
        <td colspan="3"><h3 style="margin-top:69px;">@ViewBag.Message</h3></td>
    </tr>
    <tr>
        <td width="33%">
            @using (Html.BeginForm("csvMultipleFileUpload", "MultipleCsvFileUpload", null, FormMethod.Post, new { enctype = "multipart/form-data", id = "frmID" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                @Html.Hidden("qid",  new { @id = "id" })
                <fieldset>
                    <legend>Siemens Files Upload</legend>

                    <div class="editor-label">
                        <label>Siemens Files:</label>
                    </div>
                    <div class="editor-field">
                        <input type="file" name="fileNames" multiple="multiple" id="fileNames" />
                    </div>
                    <div>
                        <table>
                            <tr>
                               <td>
                                   @Html.Label("Select Type")
                               </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.RadioButton("rbGrp", "Delayed", isChecked:false) @Html.Label("Delayed Upload")
                                    @Html.RadioButton("rbGrp", "Instant", isChecked: false) @Html.Label("Instant Upload")
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div>
                        <p>
                            <input type="submit" value="Siemens Files Upload" id="btnSubmit" />
                        </p>
                    </div>
                </fieldset>

            }
           
            <div>
                @Html.ActionLink("Back to List", "Index")
            </div>  
        </td>
        <td width="33%">            
            @using (Html.BeginForm("DelayedUploadSingle", "DelayedUplaod", null, FormMethod.Post, new { enctype = "multipart/form-data", id = "frmID" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                <fieldset>
                    <legend>DatalogAB File upload</legend>

                    <div class="editor-label">
                        <label>DatalogAB Files:</label>
                    </div>
                    <div class="editor-field">
                        <input type="file" name="fileNames" multiple="multiple" id="fileNames" />
                    </div>
                    <div>
                        <table>
                            <tr>
                                <td>
                                    @Html.Label("Select Type")
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.RadioButton("rbGrp", "Delayed", isChecked: false) @Html.Label("Delayed Upload")
                                    @Html.RadioButton("rbGrp", "Instant", isChecked: false) @Html.Label("Instant Upload")
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div>
                        <p>
                            <input type="submit" value="DatalogAB Files Upload" id="btnSubmit" />
                        </p>
                    </div>
                </fieldset>
            }
            <div>
                @*@Html.ActionLink("Back to List", "Index")*@
            </div>
        </td>       
        <td width="33%">
            @using (Html.BeginForm("MonitouchMultipleFileUpload", "MonitouchMultipleFileupload", null, FormMethod.Post, new { enctype = "multipart/form-data", id = "frmID" }))
            {
                @Html.AntiForgeryToken()
                @Html.ValidationSummary(true)
                <fieldset>
                    <legend>Monitouch Files Upload</legend>

                    <div class="editor-label">
                        <label>Monitouch Files:</label>
                    </div>
                    <div class="editor-field">
                        <input type="file" name="fileNames" multiple="multiple" id="fileNames" />
                    </div>
                    <div>
                        <table>
                            <tr>
                                <td>
                                    @Html.Label("Select Type")
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.RadioButton("rbGrp", "Delayed", isChecked: false) @Html.Label("Delayed Upload")
                                    @Html.RadioButton("rbGrp", "Instant", isChecked: false) @Html.Label("Instant Upload")
                                </td>
                            </tr>
                        </table>
                    </div>
                    <div>
                        <p>
                            <input type="submit" value="Monitouch Files Upload" id="btnSubmit" />
                        </p>
                    </div>
                    
                </fieldset>

            }
            
            <div>
                @*@Html.ActionLink("Back to List", "Index")*@
            </div>
        </td>
    </tr>
</table>
@section Scripts {
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
}
